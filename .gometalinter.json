  ],
  "Disable" :[
	"dupl",
	"gas"
  ],
  "Exclude" : [
  	 ".*Errors unhandled.*fmt.Fprint.*gasv2.*"
  ],
  "Linters": {
        "gasv2": {
          	"Command": "gas -fmt=csv",
          	"Pattern": "^(?P<path>.*?\\.go),(?P<line>\\d+),(?P<message>[^,]+,[^,]+,[^,]+,\".*\")"
        }
   },
 "LineLength" : 90,
 "Deadline": "100s"
}