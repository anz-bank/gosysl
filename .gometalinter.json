{
  "Enable": [
  	"deadcode", 
  	"unconvert",
	"testify",
	"test",
	"gasv2",
	"gofmt",
	"goimports",
	"lll",
	"misspell",
	"nakedret",
	"unparam",
	"safesql",
	"megacheck",
	"vet",
	"vetshadow",
	"gotype",
	"gotypex",
	"deadcode",
	"gocyclo",
	"golint",
